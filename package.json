    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "css": "./node_modules/.bin/tailwindcss -i ./src/styles/index.css -o ./src/styles/output.css",
    "css:watch": "npm run css -- --watch",
    "antlr4": "node ./antlr/antlr4.js",
    "start": "npm run css && react-scripts start -- --port=3000",
    "build": "npm run css && react-scripts build",
    "lint": "./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/eslint ./src --ext .ts,.tsx,.js,.jsx",
    "jest": "CI=true react-scripts test --all",
    "test": "npm run lint && npm run jest",
    "deploy": "./node_modules/.bin/firebase deploy --only hosting"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
